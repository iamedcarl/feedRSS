<script type="text/javascript">
  <% if logged_in? %>
    window.currentUser = <%= render(
      "api/users/user.json.jbuilder",
      user: current_user
    ).html_safe %>
  <% end %>

  window.icon = '<%= image_path 'icon.png' %>'
  window.headerIcon = '<%= image_path 'icon_header.png' %>'
</script>

<main id=root></main>
